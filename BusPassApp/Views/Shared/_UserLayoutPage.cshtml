    @*Signin popup*@
    <div class="modal in" id="LoginModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-modal="true">
        <div class="modal-dialog" role="document" style="width: 34%;">
            <div class="modal-content">
                <div class="login-form dvclslgndetails">
                    <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span></button>
                    <div class="avatar"><i class="fa fa-user-o"></i></div>
                    <h4 class="modal-title">Login to Your Account</h4>
                    <div class="form-group">
                        <input type="email" class="form-control" name="Mail id" data-mandatory="Y" id="txtlgnmailid" placeholder="Mail id" required="required">
                    </div>
                    <div class="form-group">
                        <input type="password" class="form-control" name="Password" data-mandatory="Y" id="txtlgnpassword" placeholder="Password" required="required">;
                    </div>
