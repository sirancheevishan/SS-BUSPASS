    var RedirectToHomePath = "@Url.Action("HomePage", "HomePage")";
    var HomeURL = "@Url.Action("Home", "Home")";
    var Verify_RegistrationURL = "@Url.Action("Verify_Registration", "Home")";
    var RedirectURL = '@Url.Action("HomePage", "HomePage")';
    var ComingsoonURL = "@Url.Action("ComingSoon", "Home")";
    var ContactusURL = "@Url.Action("ContactUs", "Home")";
    var AboutusURL = "@Url.Action("AboutUs", "Home")";
    var GetPassDetails = "@Url.Action("GetPassDetails", "HomePage")";
    var GetSlctdPassDetails = "@Url.Action("GetSelectedPassDetails", "BusPass")";
    var BookingURL = "@Url.Action("IssueNewPass", "HomePage")";
    var ExisitingURL = '@Convert.ToString(Session["EXISITING_USER"])';
    var newUserHomeurl = "@Url.Action("PassInfo", "HomePage")";
    var ExisitingHomeurl = "@Url.Action("ExisitingUser", "HomePage")";

    $(document).ready(function () {
        $("#spnUserName").html('@strUserName');
    });


    function gotoHome() {
        if (ExisitingURL == 'Y') {
            window.location.href = ExisitingHomeurl;
        }
        else {
            window.location.href = newUserHomeurl;
        }
    }





</script>
