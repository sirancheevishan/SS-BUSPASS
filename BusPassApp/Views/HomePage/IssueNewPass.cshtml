@{
    ViewBag.Title = "IssueNewPass";
    Layout = "~/Views/Shared/_UserLayoutPage.cshtml";
    string strUserData = Session["USER_DATA"] != null ? Session["USER_DATA"].ToString() : "";
    string type = Convert.ToString(Session["Pass_type"]);
    string RenewalFlag = Convert.ToString(Session["EXISITING_USER"]);
    string strFName = RenewalFlag != "Y" ? Convert.ToString(Session["USER_NAME"]): Convert.ToString(Session["RE_USER_NAME"]);
    string strLastName = RenewalFlag != "Y" ? Convert.ToString(Session["USER_FATHER_NAME"]) : Convert.ToString(Session["RE_USER_LNAME"]);
    string strDOB = RenewalFlag != "Y" ? Convert.ToString(Session["USER_DOB"]) : Convert.ToString(Session["RE_USER_DOB"]);
    //  string strSex = RenewalFlag != "Y" ? Convert.ToString(Session["USER_SEX"]) : Convert.ToString(Session["RE_TITLE"]);
    string strSex = Convert.ToString(Session["USER_SEX"]);
    string strNum = RenewalFlag != "Y" ? Convert.ToString(Session["USER_NUM"]) : Convert.ToString(Session["RE_USER_NUM"]);
    string strMail = RenewalFlag != "Y" ? Convert.ToString(Session["USER_MAIL"]) : Convert.ToString(Session["RE_USD_USER_MAILID"]);
    string strAmount = RenewalFlag != "Y" ? Convert.ToDouble(Session["PassAmount"]).ToString() : Convert.ToDouble(Session["RE_Amount"]).ToString();
    string strEffectFrom = Convert.ToString(Session["RE_Effective_From"]);
    string strEffectTo = Convert.ToString(Session["RE_Effective_To"]);
}

